set terminal postscript eps enhanced color font ',12'
set output 'impoverss.eps'
unset key
set style data pm3d
set xlabel 'Budget Level' 
set ylabel 'Problem Index'
set title 'Average MED Improvement Percentage'
set auto x
set auto y
set auto z
set grid
set pm3d implicit at s
set palette model HSV
set palette rgb 3,2,2
splot 'impoverss.dat'
